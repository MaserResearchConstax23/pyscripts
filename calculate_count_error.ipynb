    "import math\n",
    "def div_error(x,y):\n",
    "    return (x/y) * (math.sqrt((1/x)+(1/y)))\n",
    "\n",
    "x,y=2,9\n",
    "print(div_error(x,y))\n",
    "print(str(round(x/y,3))+\" +/- \"+str(round(div_error(x,y),3)))\n",
    "print(str(round(round(x/y,3)*100,1))+\"% +/- \"+str(round(round(div_error(x,y),3)*100,1))+\"%\")"
   ]
  }
